{
  "manifest_version": 2,
  "name": "Nanolist",
  "description": "List from webpages selection for abz.agency",
  "author": "Aleksey Chipiga",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'unsafe-eval'; object-src 'self';"
}